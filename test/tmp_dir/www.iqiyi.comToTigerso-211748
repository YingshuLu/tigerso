   function openFocusAd(c){if(!Q.browser.iPad){if(c&&c.type=="play"){for(var b=0,a=adData.length;b<a;b++){if(adData[b].frame==c.frame){var d=getFlashStr("1004","411",adData[b].url);document.getElementById("1000000000616").innerHTML=d;document.getElementById("1000000000616").style.display="block";return true}}}}return false};if(!Q.browser.iPad&&focusAd.c){var d=new Date();var s=d.getFullYear()+"/"+(parseInt(d.getMonth())+1)+"/"+(parseInt(d.getDate())+1)+" 00:00:00";s=new Date(Date.parse(s))-d;var flag=Q.cookie.get(focusAd.f);var count=Q.cookie.get(focusAd.c);count=count==null?1:count;if(count<=focusAd.u.length*2){var adUrl="";if(!flag){var index=Math.round((focusAd.u.length-1)*Math.random());adUrl=focusAd.u[index];Q.cookie.set(focusAd.f,index,{path:"/",expires:s,domain:"iqiyi.com"})}else{flag=parseInt(flag);if(count%2==0&&focusAd.u.length==2){adUrl=focusAd.u[1-flag]}else{adUrl=focusAd.u[flag]}}var adHtml=getFlashStr(focusAd.w,focusAd.h,adUrl);document.getElementById("1000000000616").innerHTML=adHtml;document.getElementById("1000000000616").style.display="block";Q.cookie.set(focusAd.c,++count,{path:"/",expires:s,domain:"iqiyi.com"})}}function closeCrazy(b){try{Q.external.launchBgAd()}catch(a){}var b="1000000000616";document.getElementById(b).style.display="none";document.getElementById(b).innerHTML=""}function getFlashStr(b,a,e){var c='<object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000"';c+='width="'+b+'" height="'+a+'" align="middle" id="'+Math.random()+'">';c+='<param name="movie" value="'+e+'" />';c+='<param name="quality" value="high" />';c+='<param name="wmode" value="transparent" />';c+='<param name="allowscriptaccess" value="always" />';c+='<embed src="'+e+'" quality="high" width="'+b+'" height="'+a+'" align="middle" wmode="transparent" allowscriptaccess="always" type="application/x-shockwave-flash" /></object>';return c};
