      <!--该脚本代码依赖Q，所以放到sea.js后面-->
<script type="text/javascript">
    (function(rdoms, ckname){
          if(!rdoms || rdoms.length === 0){
            return;
          }
          if(rdoms.length > 1){
            rdoms = Q.$(rdoms[0]);
          }
          var nameArr = [];
          var showAdNum = parseInt(rdoms.attr('data-shownum'),10);/* 广告显示测试，0就是无限次 */
          var adNum = 1;
          var params = JSON.parse(Q.cookie.get(ckname));     
          function hideJingDongAd(){  
            rdoms.addClass('dn');
            if(_adArea) {
            	_adArea.addClass('dn');
                var _parent = _adArea.parent();
                _parent.removeClass('skin_focus');
                _parent.parent().removeClass('skin_inner');
            }            
            if(adSkinDom) {
               adSkinDom.removeClass('skin_inner')
            }  
            
            _body.removeClass('skin_wrap');
          }
          if(rdoms.attr('data-adnum')){
            adNum = parseInt(rdoms.attr('data-adnum'),10);
            for(var i=0;i<adNum;i++){
              nameArr.push('skinad'+i);
            }
          } 
          if(adNum>1){
            if (!params) {
              params = {};
              var len = nameArr.length;
              var rdNum = len*Math.random();
              params.rdNum = Math.floor(rdNum);
              params.index = 0;
            }else {          
                var rdNum = parseInt(params.rdNum,10);
                if(rdNum%2 === 1) {
                    params.rdNum = 0;
                    params.index++;
                }else{
                   params.rdNum = 1;
                   params.index++;
                }
            }
          }else if(adNum==1){
            if(!params){
               params = {};
               params.rdNum = 0;
               params.index = 0;
            }else{
               params.rdNum = 0;
               params.index++;
            }       
          }    
          var _body = Q.$('body');
          var _focusInner = Q.$('#adSkinInner');
          var _adArea = Q.$('#jingdongArea');
          var xclose = Q.$('#closeJingdongAd') || "";
          var adSkinDom = Q.$('#adSkinInner');
          var skinadName = 'skin_ad'+params.rdNum;
          if(_adArea) {
          	_adArea.attr('href',_adArea.attr('data-elem-url'+params.rdNum));
          } 
          
          var indexNum = adNum*showAdNum;
          if(indexNum===0){
            indexNum = 1000;/* showAdNum为 0 则表示无限次，JS手动赋值100即可，不必再次操作cookie */
          }
          if (!params || params.index < indexNum) {
              _focusInner.addClass(skinadName);  //选择哪个样式
              Q.cookie.set(ckname, JSON.stringify(params), {path: '/', domain: 'iqiyi.com', "expires": 24 * 3600 * 1000});
              if(!adSkinDom.hasClass('skin_inner')) {
            	adSkinDom.addClass('skin_inner');	      
              }   
              if(rdoms.hasClass('dn')) {
                  rdoms.removeClass('dn');
              }
              if(xclose){
                xclose.on('click', function (e) {
                    Q.event.get(e).preventDefault();
                    hideJingDongAd();
                    var currCookie = JSON.parse(Q.cookie.get(ckname)); 
                    if(currCookie){
                      params.index = indexNum;
                      Q.cookie.set(ckname, JSON.stringify(params), {path: '/', domain: 'iqiyi.com', "expires": 24 * 3600 * 1000});
                    }   
                });
              }
          } 
          //else {
          //  hideJingDongAd();
          //}
    })(Q.$('#widget-jingdongAd'), 'jingdong');
</script>      <div class="index-footer">
