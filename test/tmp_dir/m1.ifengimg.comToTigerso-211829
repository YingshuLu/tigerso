(function() {
    var getCookie = function(N) {
        var c = document.cookie.split("; ");
        for (var i = 0; i < c.length; i++) {
            var d = c[i].split("=");
            if (d[0] == N) {
                return unescape(d[1]);
            }
        }
        return "";
    };
    var getNavCityA = function(href) {
        var a_all = document.links;
        var len = a_all.length;
        for (var i = 0; i < len; i++) {
            if (a_all[i].href == href) {
                return a_all[i];
                break;
            }
        }
        return '';
    }
    var getNavCityAById = function(id, href) {
        var a_all = document.getElementById(id).getElementsByTagName('a');
        var len = a_all.length;
        for (var i = 0; i < len; i++) {
            if (a_all[i].href == href) {
                return a_all[i];
                break;
            }
        }
        return '';
    }
    var prov = getCookie('prov');
    var city = getCookie('city');
    var ip = getCookie('region_ip');
    var href = '';
    var inner = '';
    var id = "viceNav";
    var haveZhan = document.getElementById(id) ? false : true;
    if (prov == 'cn0551') {
        href = "http://ah.ifeng.com";
        inner = "安徽" + (haveZhan ? "站" : "");
    } else if (prov == 'cn0791') {
        href = "http://jx.ifeng.com";
        inner = "江西" + (haveZhan ? "站" : "");
    } else if (prov == "cn025") {
        href = "http://js.ifeng.com";
        inner = "江苏" + (haveZhan ? "站" : "");
    } else if (prov == "cn027") {
        href = "http://hb.ifeng.com";
        inner = "湖北" + (haveZhan ? "站" : "");
    } else if (prov == "cn0311") {
        href = "http://hebei.ifeng.com/";
        inner = "河北" + (haveZhan ? "站" : "");
    } else if (prov == "cn0898" || ip.indexOf('36.1.135') >= 0) {
        href = "http://hainan.ifeng.com";
        inner = "海南" + (haveZhan ? "站" : "");
    } else if (prov == "cn0731") {
        href = "http://hunan.ifeng.com";
        inner = "湖南" + (haveZhan ? "站" : "");
    } else if (prov == "cn022") { //    天津
        href = "http://tj.ifeng.com";
        inner = "天津" + (haveZhan ? "站" : "");
    } else if (prov == "cn023") { //    重庆
        href = "http://cq.ifeng.com";
        inner = "重庆" + (haveZhan ? "站" : "");
    } else if (prov == "cn028") { //    四川
        href = "http://sc.ifeng.com";
        inner = "四川" + (haveZhan ? "站" : "");
    } else if (prov == "cn029") { //陕西
        href = "http://sn.ifeng.com";
        inner = "陕西" + (haveZhan ? "站" : "");
    } else if (prov == "cn0371") { //河南
        href = "http://hn.ifeng.com";
        inner = "河南" + (haveZhan ? "站" : "");
    } else if (prov == "cn024") { //辽宁
        href = "http://ln.ifeng.com";
        inner = "辽宁" + (haveZhan ? "站" : "");
    } else if (prov == "cn020") { //    广州(广东)
        href = "http://gd.ifeng.com";
        inner = "广东" + (haveZhan ? "站" : "");
    } else if (prov == "cn0531") { //山东
        if (city == "0532") { //青岛
            href = "http://qd.ifeng.com";
            inner = "青岛" + (haveZhan ? "站" : "");
        } else {
            href = "http://sd.ifeng.com";
            inner = "山东" + (haveZhan ? "站" : "");
        }
    } else if (prov == 'cn0591') { //
        if (city == '0592') {
            href = "http://xm.ifeng.com";
            inner = "厦门" + (haveZhan ? "站" : "");
        } else {
            href = "http://fj.ifeng.com";
            inner = "福建" + (haveZhan ? "站" : "");
        }
    } else if (prov == 'cn0931' || prov == 'cn0971' || prov == 'cn0951') {
        href = "http://xibei.ifeng.com";
        inner = "西北" + (haveZhan ? "站" : "");
    } else if (prov == 'cn0451') {
        href = 'http://hlj.ifeng.com';
        inner = (haveZhan ? "黑" : "黑") + '龙江';
    } else if (prov == 'cn0431') {
        href = 'http://jl.ifeng.com';
        inner = "吉林" + (haveZhan ? "站" : "");
    } else if (prov == 'cn0571' || city == "0571") {
        href = 'http://zj.ifeng.com/index.shtml';
        inner = "浙江" + (haveZhan ? "站" : "");
    }
    if (haveZhan) {
        var navCityA = getNavCityA("http://city.ifeng.com/special/duihua/");
    } else {
        var navCityA = getNavCityAById(id, "http://imall.ifeng.com/");
    }
    if (navCityA != '') {
        if (href != '' && inner != '') {
            navCityA.href = href;
            navCityA.innerHTML = inner;
        }
    }
})();
